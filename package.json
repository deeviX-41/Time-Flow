{
  "name": "time-f-prisma",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "bin": {
    "timef": "./dist/bin/timef.js"
  },
  "scripts": {
    "build": "nest build",
    "build:cli": "tsc -p tsconfig.json",
    "build:all": "npm run build && npm run build:cli",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "cli": "ts-node -r tsconfig-paths/register ./bin/timef.ts",
    "cli:dev": "ts-node -r tsconfig-paths/register ./bin/timef.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/client": "^6.16.2",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "dotenv": "^16.4.5",
    "inquirer": "^12.9.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@types/express": "^5.0.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^22.10.7",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "prettier": "^3.4.2",
    "prisma": "^6.16.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
