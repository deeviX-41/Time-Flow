{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "outDir": "./dist",
    "rootDir": "./",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["bin/**/*", "cli/**/*", "src/**/*.service.ts", "src/prisma/**/*"],
  "exclude": ["node_modules", "dist", "test", "src/main.ts"]
}
